.levelVip-wrap
  background #f8f8f8
  padding: 64px 0
  .section
    width 1200px
    font-size: 16px;
    margin: 70px auto 36px;
    &:first-child
      margin-top: 0
    &:last-child
      margin-bottom: 0
    .title
      line-height: 1
      font-size: 20px;
      color: #c0a15d
      padding-left: 18px
      border-left 2px solid #c0a15d
      font-weight bold
      margin-bottom 36px
    .content
      line-height 24px;
      font-size 16px;
      color: #808080;
      width: 1158px;
      margin: 0 auto 36px;
      &:last-child
        margin-bottom 0
    table
      width: 1158px;
      margin: auto;
      border-right: 1px solid #d9d9d9;
      border-bottom: 1px solid #d9d9d9
      tr
        height 50px
        td
          vertical-align: middle;
          text-align: center;
          border-left: 1px solid #d9d9d9;
          border-top: 1px solid #d9d9d9;
          font-size: 16px
          color: #333333;
          &.bg
            background: #c0a15d
            color: #ffffff
            border: 0

      &.table2
        tr
          td
            &.width25
              width 27%

    .s-title
      padding-left: 21px
      color: #333333
      font-size: 16px;
      font-weight: bold
      margin-bottom: 9px;
