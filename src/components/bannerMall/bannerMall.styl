*
  margin 0
  padding 0
  box-sizing border-box
  font-family MicrosoftYaHei

.banner-wrap
  position relative
  .swiper-container
    width 100%
    height 400px

  .swiper-wrapper
    width 100%
    height 100%
  .swiper-slide
    background-position: center;
    background-repeat: no-repeat;
  .limit
    width: 1200px;
    height: 0;
    margin: auto
    position: absolute
    left 0
    right 0
    top: 0
