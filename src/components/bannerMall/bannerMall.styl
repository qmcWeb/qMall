*
  margin 0
  padding 0
  box-sizing border-box
  font-family MicrosoftYaHei

.banner-wrap
  .swiper-container
    width 100%
    height 400px
  .swiper-wrapper
    width 100%
    height 100%
  .swiper-slide
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
    img
      width 100%
      height 100%
  .swiper-pagination-bullet
    width 10px
    height 10px
    display inline-block
    background: #7c5e53
